{{#each @contestant as |teamate|}}
<div class="table-container">
    <h3>{{teamate.name}}</h3>
    <table>
        <thead>
            <tr>
                <td>Teams</td>
                <td>Week 1</td>
                <td>Week 2</td>
                <td>Week 3</td>
                <td>Week 4</td>
                <td>Week 5</td>
                <td>Team Total</td>
                <td>Group Total</td>
            </tr>
        </thead>
        <tbody>
            {{#each teamate.teamates as |user index|}}
                <tr>               
                    <td>{{user.fname}} {{user.lname}}</td>
                    {{#each user.scores as |score|}}
                    <td>{{score}}</td>
                    {{/each}}
                    <td>557</td>
                    {{#unless index}}
                    <td rowspan="6">781</td>
                    {{/unless}}
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>
{{/each}}